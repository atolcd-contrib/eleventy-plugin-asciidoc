{
  "name": "eleventy-plugin-asciidoc",
  "version": "3.0.1",
  "description": "Adds support for AsciiDoc to Eleventy",
  "main": ".eleventy.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com:saneef/eleventy-plugin-asciidoc.git"
  },
  "scripts": {
    "lint": "eslint lib/**.js tests/**.js",
    "test": "nyc ava -v --color",
    "prepare": "husky install"
  },
  "keywords": [
    "asciidoc",
    "asciidoctor",
    "eleventy",
    "11ty",
    "eleventy-plugin"
  ],
  "author": "Saneef Ansari <hello@saneef.com> (https://saneef.com/)",
  "license": "MIT",
  "dependencies": {
    "asciidoctor": "^3.0.2",
    "debug": "^4.3.4",
    "gray-matter": "^4.0.3",
    "nunjucks": "^3.2.4"
  },
  "devDependencies": {
    "ava": "^5.3.1",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-xo-space": "^0.34.0",
    "eslint-plugin-prettier": "^5.0.0-alpha.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "nyc": "^15.1.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,md,json}": "prettier --write"
  },
  "ava": {
    "files": [
      "tests/**/*",
      "!tests/utils.js"
    ],
    "ignoredByWatcher": [
      "tests/output/**"
    ]
  }
}
